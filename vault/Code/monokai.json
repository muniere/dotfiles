{
  "name": "Monokai",
  "type": "dark",
  "colors": {
    "editor.background": "#272822",
    "editor.foreground": "#f6f6f6",
    "editor.lineHighlightBackground": "#3e3d32",
    "editor.selectionBackground": "#214283",
    "editorCursor.foreground": "#bbbbbb",
    "editorWhitespace.foreground": "#505050",
    "editorIndentGuide.background": "#373737",
    "editorIndentGuide.activeBackground": "#505050",
    "editor.findMatchBackground": "#e4e4e4",
    "editor.findMatchHighlightBackground": "#e4e4e4",
    "editorLineNumber.foreground": "#8f908a",
    "editorLineNumber.activeForeground": "#c2c2bf",
    "editorBracketMatch.background": "#595959",
    "editorBracketMatch.border": "#595959",
    "editorError.foreground": "#ff0000",
    "editorWarning.foreground": "#fff600",
    "editorInfo.foreground": "#808080",
    "diffEditor.insertedTextBackground": "#294436",
    "diffEditor.removedTextBackground": "#484a4a",
    "statusBar.background": "#414339",
    "statusBar.foreground": "#f6f6f6",
    "activityBar.background": "#272822",
    "activityBar.foreground": "#f6f6f6",
    "sideBar.background": "#272822",
    "sideBar.foreground": "#f6f6f6"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#74715e"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#e6da74"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#ae81ff"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#f92772"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#a7e22e"
      }
    },
    {
      "scope": ["entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#66d9ee"
      }
    },
    {
      "scope": ["variable", "variable.language"],
      "settings": {
        "foreground": "#f6f6f6"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#fe9720"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#f92772"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#a6e22e"
      }
    }
  ]
} 
