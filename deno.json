{
  "fmt": {
    "lineWidth": 100
  },
  "imports": {
    "cliffy/": "https://deno.land/x/cliffy@v0.25.4/",
    "eta/": "https://deno.land/x/eta@v1.12.3/",
    "deno/": "https://deno.land/std@0.167.0/"
  },
  "tasks": {
    "build": "deno compile --allow-run --allow-read --allow-write --allow-env --output dist/ app/cli.ts",
    "list": "deno run --allow-run --allow-read --allow-write --allow-env app/cli.ts list",
    "link": "deno run --allow-run --allow-read --allow-write --allow-env app/cli.ts link",
    "unlink": "deno run --allow-run --allow-read --allow-write --allow-env app/cli.ts unlink",
    "relink": "deno task unlink && deno task link",
    "cleanup": "deno run --allow-run --allow-read --allow-write --allow-env app/cli.ts cleanup"
  }
}
