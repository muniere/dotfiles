# Flutter
if [ -d /usr/local/opt/flutter/bin/cache/dart-sdk/bin/ ]; then
  addpath /usr/local/opt/flutter/bin /usr/local/opt/flutter/bin/cache/dart-sdk/bin
elif [ -d /usr/local/opt/flutter ]; then
  addpath /usr/local/opt/flutter/bin
fi

if [ -d $HOME/.pub-cache/bin ]; then
  addpath $HOME/.pub-cache/bin
fi

