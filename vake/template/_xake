<% BSLASH = "\\" %>\
#compdef xake

function _xake() {
  _arguments -C ${BSLASH}
    % for option in options:
    '(${" ".join(option.option_strings)})'{${",".join(option.option_strings)}}'[${option.help}]' ${BSLASH}
    % endfor
    '1: :->actions' && ret=0

  case "$state" in
    actions)
      local -a actions
      actions=(${" ".join(actions)})
      _describe 'actions' actions && ret=0
      ;;

  esac

  return ret
}

# vim: ft=sh ts=2 sts=2 sw=2
